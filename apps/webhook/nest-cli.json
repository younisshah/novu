{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": ".env",
        "outDir": "dist/src"
      },
      {
        "include": ".env.development",
        "outDir": "dist/src"
      },
      {
        "include": ".env.test",
        "outDir": "dist/src"
      },
      {
        "include": ".env.production",
        "outDir": "dist/src"
      }
    ],
    "plugins": [],
    "webpack": true
  }
}

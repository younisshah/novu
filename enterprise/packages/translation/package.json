    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "multer": "^1.4.5-lts.1",
    "shortid": "^2.2.16",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
